{
  "name": "auth-worker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cd ../.. && npx wrangler dev --config apps/auth/wrangler.jsonc --persist-to .wrangler/state",
    "dev:remote": "cd ../.. && npx wrangler dev --config apps/auth/wrangler.jsonc --remote",
    "build": "tsc",
    "deploy": "wrangler deploy"
  },
  "dependencies": {
    "@repo/db": "*",
    "@repo/utils": "*",
    "hono": "^4.6.14"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "typescript": "^5.6.3",
    "wrangler": "^3.86.1"
  }
}
